{
  "name": "koliveira15",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/koliveira15/nx-sonarqube.git"
  },
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "build-reference-docs": "node ./tools/build-tool-docs.js",
    "serve-reference-docs": "nodemon ./tools/build-tool-docs.js",
    "test": "nx test",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "private": true,
  "devDependencies": {
    "@nx/devkit": "18.1.2",
    "@nx/eslint": "18.1.2",
    "@nx/eslint-plugin": "18.1.2",
    "@nx/jest": "18.1.2",
    "@nx/js": "18.1.2",
    "@nx/plugin": "18.1.2",
    "@nx/webpack": "18.1.2",
    "@nx/workspace": "18.1.2",
    "@swc-node/register": "1.8.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.96",
    "@types/jest": "29.4.4",
    "@types/node": "^18.16.9",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "28.1.3",
    "jsonc-eslint-parser": "2.1.0",
    "lint-staged": "13.0.3",
    "nodemon": "^3.1.0",
    "nx": "18.1.2",
    "prettier": "2.6.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.3.3",
    "verdaccio": "^5.0.4"
  },
  "dependencies": {
    "@astrojs/check": "0.5.2",
    "@astrojs/starlight": "0.21.1",
    "@phenomnomnominal/tsquery": "5.0.1",
    "@swc/helpers": "0.5.3",
    "astro": "4.3.5",
    "sonarqube-scanner": "3.1.0",
    "tslib": "2.3.0"
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "18.0.3",
    "@nx/nx-darwin-x64": "18.0.3",
    "@nx/nx-linux-x64-gnu": "18.0.3",
    "@nx/nx-win32-x64-msvc": "18.0.3"
  },
  "lint-staged": {
    "*.{js,json,md,ts}": [
      "npx nx format:write --uncommitted"
    ]
  },
  "nx": {
    "includedScripts": [
      "build-reference-docs",
      "serve-reference-docs"
    ],
    "targets": {
      "build-reference-docs": {
        "cache": true,
        "inputs": [
          "{workspaceRoot}/tools/build-tool-docs.js",
          "{workspaceRoot}/packages/nx-sonarqube/src/executors",
          "{workspaceRoot}/packages/nx-sonarqube/src/generators"
        ],
        "outputs": [
          "{workspaceRoot}/apps/docs-site/src/content/docs/reference/Executors",
          "{workspaceRoot}/apps/docs-site/src/content/docs/reference/Generators"
        ]
      }
    }
  }
}
